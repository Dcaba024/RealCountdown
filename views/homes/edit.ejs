<%- include("../partials/header") %>

<div style="text-align: center;">
  Edit <%= home.address %>
</div>


<div style="width: 30%; margin: 50px auto;">
    <form action="/homes/<%=home._id %>/?_method=PUT" method="POST">
        <div class="form-group">
            <input class="form-control" type="text" name="home[address]" value="<%= home.address %>">
        </div>


        <!-- new addition -->

        <div class="form-group">
            <input class="form-control" type="text" name="home[street]" value="<%= home.street %>">
        </div>

        <div class="form-group">
            <input class="form-control" type="text" name="home[city]" value="<%= home.city %>">
        </div>

        <div class="form-group">
            <input class="form-control" type="text" name="home[state]" value="<%= home.state %>">
        </div>

        <div class="form-group">
            <input class="form-control" type="text" name="home[zip]" value="<%= home.zip %>">
        </div>

        <div class="form-group">
            <input class="form-control" type="text" name="home[price]" value="<%= home.price %>">
        </div>

       
        <!-- new addition ends here -->
        
        <div class="form-group">
            <input class="form-control" type="text" name="home[Image]" value="<%= home.Image %>">
        </div>
        

        <div class="form-group">
            <input class="form-control" type="text" name="home[description]" value="<%= home.description %>"id="">
        </div>
        
        
        <button class="btn btn-lg btn-block btn-outline-dark">Submit!</button>
    </form>
</div>


<%- include("../partials/footer") %>