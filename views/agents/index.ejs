<%- include("../partials/header") %>

<div class="container">

    <div class="row">
        <div class="column" id="agent-table">
            <h1>Seller</h1>
            <div id="agent-table">
                <img src="<%= currentUser.image %>" alt="">
            </div>
        </div>
        <div class="column" id="agent-table">
            <h1>Agents</h1>
        </div>
    </div>



    <% Agents.forEach(function(agent){ %>

        <div class="row">
            <div id="agent-table">
              
            </div>
            <div id="agent-table">
                <div id="icard">
           
                    <li class="card">
                        <div class="d-flex justify-content-start">
                            <img class="pic" src="<%= agent.image %>" alt="" height="100%" width="100%">
                        </div>
                        <p id="par-card"><%=agent.name %> <%= agent.lastname %></p>
                        <p><%= agent.phoneNumber %></p>
        
                        <a href="/agents/<%= agent._id %>/new" id="view-bid" class="btn btn-primary">Start Countdown</a>
                    </li>
        
                </div>

            </div>
           
    

        </div>

        
      
        
        
        
        <% }) %>
</div>






<%- include("../partials/footer") %>