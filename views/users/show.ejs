<%- include("../partials/header") %>

<div class="container">
    <div class="row" id="mycountdown">
        <h1>My Countdown</h1>
    </div>
    
    
    <div class="row text-center" style="display: flex; flex-wrap: wrap;">
        <% homes.forEach(function(home){ %>
            
            <div class="column">
                <div class="">
                    <img src="<%=home.Image %>" alt="" height="100%" width="100%">
                    <div class="caption">
                        <h3><%= home.author.username %>'s home for sale</h3>
                    </div>
                    <p>
                        <a href="/homes/<%=home._id%>" class="btn btn-primary btn-large">View bid</a>
                    </p>
                   
                </div>
            </div>
        <% }) %>
    </div>



</div>




<%- include("../partials/footer") %>